<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <record model="ir.actions.server" id="action_refresh_product_product_spt">
            <field name="name">Refresh Images Variant</field>
            <field name="model_id" ref="tzc_product_import_spt.model_product_product"/>
            <field name="binding_model_id" ref="tzc_product_import_spt.model_product_product" />
            <field name="state">code</field>
            <field name="code">
                action = records.refresh_images_product()
            </field>
        </record>

        <record model="ir.actions.server" id="action_refresh_product_template_spt">
            <field name="name">Refresh Images Template</field>
            <field name="model_id" ref="tzc_product_import_spt.model_product_template"/>
            <field name="binding_model_id" ref="tzc_product_import_spt.model_product_template" />
            <field name="state">code</field>
            <field name="code">
                action = records.refresh_images_template()
            </field>
        </record>

    </data>
</odoo>